<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="FlysystemResource">
        <property name="type" type="string"/>
        <property name="path" type="string"/>
        <property name="timestamp" type="int"/>
        <property name="size" type="int"/>
        <property name="dirname" type="string"/>
        <property name="basename" type="string"/>
        <property name="extension" type="string"/>
        <property name="filename" type="string"/>
        <property name="isFile" type="bool"/>
    </transfer>

    <transfer name="FlysystemResourceMetadata">
        <property name="type" type="string"/>
        <property name="path" type="string"/>
        <property name="timestamp" type="int"/>
        <property name="size" type="int"/>
        <property name="isFile" type="bool"/>
    </transfer>

    <transfer name="FlysystemConfig">
        <property name="name" type="string"/>
        <property name="type" type="string"/>
        <property name="adapterConfig" type="array" singular="adapterConfig"/>
        <property name="flysystemConfig" type="array" singular="flysystemConfig"/>
    </transfer>

    <transfer name="FileSystemQuery">
        <property name="fileSystemName" type="string"/>
        <property name="path" type="string"/>
    </transfer>

    <transfer name="FileSystemResourceMetadata">
    </transfer>

    <transfer name="FileSystemList">
        <property name="fileSystemName" type="string"/>
        <property name="path" type="string"/>
        <property name="recursive" type="bool"/>
    </transfer>

    <transfer name="FileSystemResource">
    </transfer>

    <transfer name="FileSystemStream">
        <property name="fileSystemName" type="string"/>
        <property name="path" type="string"/>
        <property name="config" type="array" singular="config"/>
    </transfer>

    <transfer name="FileSystemVisibility">
        <property name="fileSystemName" type="string"/>
        <property name="path" type="string"/>
    </transfer>

    <transfer name="FileSystemCreateDirectory">
        <property name="fileSystemName" type="string"/>
        <property name="path" type="string"/>
        <property name="config" type="array" singular="config"/>
    </transfer>

    <transfer name="FileSystemDeleteDirectory">
        <property name="fileSystemName" type="string"/>
        <property name="path" type="string"/>
    </transfer>

    <transfer name="FileSystemCopy">
        <property name="fileSystemName" type="string"/>
        <property name="sourcePath" type="string"/>
        <property name="destinationPath" type="string"/>
    </transfer>

    <transfer name="FileSystemDelete">
        <property name="fileSystemName" type="string"/>
        <property name="path" type="string"/>
    </transfer>

    <transfer name="FileSystemContent">
        <property name="fileSystemName" type="string"/>
        <property name="path" type="string"/>
        <property name="content" type="string"/>
        <property name="config" type="array" singular="config"/>
    </transfer>

    <transfer name="FileSystemRename">
        <property name="fileSystemName" type="string"/>
        <property name="path" type="string"/>
        <property name="newPath" type="string"/>
    </transfer>

</transfers>
